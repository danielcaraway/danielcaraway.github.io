    "def get_prior(string):\n",
    "    try:\n",
    "        if 'prior prison' in string:\n",
    "            return string.split('prior prison')[1]\n",
    "#         if 'has' in string:\n",
    "#             return string.split('has')[0]\n",
    "    except:\n",
    "        return 'error'\n",
    "    \n",
    "\n",
    "df['prior_prison'] = df.apply(lambda x: get_prior(x['clean']), axis=1)"
   ]
