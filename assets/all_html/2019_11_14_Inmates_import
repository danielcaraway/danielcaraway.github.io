   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_co_d(string):\n",
    "    try:\n",
    "        if 'defendants' in string:\n",
    "            return string.split('defendants')[1]\n",
    "#         if 'has' in string:\n",
    "#             return string.split('has')[0]\n",
    "    except:\n",
    "        return 'error'\n",
    "    \n",
    "\n",
    "df['co_d'] = df.apply(lambda x: get_co_d(x['clean']), axis=1)"
   ]
  },
