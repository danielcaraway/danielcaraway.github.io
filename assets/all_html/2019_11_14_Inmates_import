    "def get_dob_2(string):\n",
    "    try:\n",
    "        if 'received' in string:\n",
    "            return string.split('received')[0]\n",
    "        if 'has' in string:\n",
    "            return string.split('has')[0]\n",
    "    except:\n",
    "        return 'error'\n",
    "    \n",
    "\n",
    "df['dob_2'] = df.apply(lambda x: get_dob_2(x['dob']), axis=1)"
   ]
