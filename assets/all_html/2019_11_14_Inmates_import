   "source": [
    "def get_dob(string):\n",
    "    try:\n",
    "        if 'dob' in string:\n",
    "            return string.split('dob')[1]\n",
    "        if 'pop' in string:\n",
    "            return string.split('pop')[1]\n",
    "    except:\n",
    "        return 'error'\n",
    "    \n",
    "\n",
    "df['dob'] = df.apply(lambda x: get_dob(x['clean']), axis=1)"
   ]
