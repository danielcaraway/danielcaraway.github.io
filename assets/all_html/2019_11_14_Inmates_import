   "cell_type": "code",
   "execution_count": 37,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_edu(string):\n",
    "    try:\n",
    "        if 'education level' in string:\n",
    "            return string.split('education level')[1]\n",
    "        if 'education' in string:\n",
    "            return string.split('education')[1]\n",
    "    except:\n",
    "        return 'error'\n",
    "    \n",
    "\n",
    "df['edu'] = df.apply(lambda x: get_edu(x['clean']), axis=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "metadata": {},
   "outputs": [
