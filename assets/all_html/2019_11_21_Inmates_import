    "import re\n",
    "df['clean'] = df.apply(lambda x: re.sub(r'[\\W_]+', ' ', x[0].lower()), axis=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_dob(string):\n",
    "    try:\n",
    "        if 'dob' in string:\n",
    "            return string.split('dob')[1]\n",
    "        if 'pop' in string:\n",
    "            return string.split('pop')[1]\n",
    "    except:\n",
    "        return 'error'\n",
    "    \n",
    "\n",
    "df['dob'] = df.apply(lambda x: get_dob(x['clean']), axis=1)"
   ]
  },
