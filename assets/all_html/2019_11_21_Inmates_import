   "cell_type": "code",
   "execution_count": 67,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_race_of_v(string):\n",
    "    try:\n",
    "        if 'race of victim' in string:\n",
    "            return string.split('race of victim')[1]\n",
    "#         if 'has' in string:\n",
    "#             return string.split('has')[0]\n",
    "    except:\n",
    "        return 'error'\n",
    "    \n",
    "\n",
    "df['race_v'] = df.apply(lambda x: get_race_of_v(x['clean']), axis=1)"
   ]
  },
  {
