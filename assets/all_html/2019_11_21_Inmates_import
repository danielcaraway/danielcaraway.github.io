    "def get_edu(summary):\n",
    "    try:\n",
    "        p = re.compile(r'(?<=level)(\\W.*?)(?=prior)')\n",
    "        return p.search(summary).group()\n",
    "    except:\n",
    "        return 'n/a'\n",
    "    \n",
    "def get_occupation(summary):\n",
    "    try:\n",
    "        p = re.compile(r'(?<=occupation)(\\W.*?)(?=\\s)')\n",
    "        print(p.search(summary).group())\n",
    "#         return p.search(summary).group()\n",
    "    except:\n",
    "        print('nope')\n",
    "#         return 'n/a'\n",
    "\n",
    "t = get_occupation(test)\n",
    "        "
   ]
