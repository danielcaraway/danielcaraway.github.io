    "df['inmate_number'] = df.apply(lambda x: x['index'].split('_')[2], axis=1)\n",
    "df['last_name'] = df.apply(lambda x: x['index'].split('_')[4].split('.')[0], axis=1)\n",
    "df['first_name'] = df.apply(lambda x: x['index'].split('_')[3], axis=1)\n",
