   "source": [
    "categories = [\"teens\", \"twenties\", \"thirty+\"]\n",
    "death_row[\"age_received\"] = pd.cut(death_row[\"age_received\"], [0, 19, 29, 99], labels = categories)\n",
    "death_row[\"age_crime\"] = pd.cut(death_row[\"age_crime\"], [0, 19, 29, 99], labels = categories)"
   ]
