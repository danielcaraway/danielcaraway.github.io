    "def get_bow_from_dict(df, column):\n",
    "    each_freq = df[column].tolist()\n",
    "    most_freq = []\n",
    "    for freq in each_freq:\n",
    "        for f in freq:\n",
    "            most_freq.append(f[0])\n",
