    "\n",
    "# # bow = get_bow_from_column(all_df, 'diy_cleaner')\n",
    "# # bow =\n",
    "from collections import Counter\n",
    "all_df['bow'] = all_df.apply(lambda x: Counter(x['tokens']), axis=1)\n",
    "all_df['bow_no_sw'] = all_df.apply(lambda x: Counter(x['no_sw']), axis=1)\n",
