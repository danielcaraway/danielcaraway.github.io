   "source": [
    "import pandas as pd\n",
    "neg_df = pd.DataFrame(neg)\n",
    "pos_df = pd.DataFrame(pos)\n",
    "pos_df['PoN'] = 'P'\n",
    "neg_df['PoN'] = 'N'\n",
    "all_df = neg_df.append(pos_df)\n",
    "all_df.reset_index(drop=True,inplace=True)\n",
    "all_df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "from collections import Counter\n",
    "from nltk.sentiment.util import *\n",
    "from nltk.tokenize import casual_tokenize\n",
    "all_df['pos'] = all_df.apply(lambda x: nltk.pos_tag(Counter(casual_tokenize(x[0]))),axis=1)\n",
    "\n",
    "def get_pos_list(tuples):\n",
    "#     print(tuples)\n",
    "    return [t[1] for t in tuples]\n",
    "    \n",
    "all_df['pos_count'] = all_df.apply(lambda x: get_pos_list(x['pos']),axis=1)\n",
    "all_df['pos_str'] = all_df.apply(lambda x: ' '.join(x['pos_count']),axis=1)\n",
    "\n",
    "# all_df['pos_no_sw'] = all_df.apply(lambda x: nltk.pos_tag(x['no_sw']),axis=1)"
   ]
  },
