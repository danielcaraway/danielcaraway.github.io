  {
   "cell_type": "code",
   "execution_count": 104,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_age_crime(summary):\n",
    "    try:\n",
    "        text = re.compile(r'(?<=age at time of offense )(\\d.*?)\\W')\n",
    "        result = text.search(summary).group().strip()\n",
    "        if len(result) < 2:\n",
    "            return 'none_listed'\n",
    "        else:\n",
    "            return result\n",
    "    except:\n",
    "        return 'none_listed'\n",
    "\n",
    "age_crime = [get_age_crime(summary) for summary in df['clean'].values]    "
   ]
  },
  {
