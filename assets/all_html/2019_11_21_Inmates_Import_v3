    "def get_vics(summary):\n",
    "    try:\n",
    "        text = re.compile(r'(male|men|man)')\n",
    "        vics = text.findall(summary)\n",
    "        print(len(vics))\n",
    "\n",
    "    except:\n",
    "        print('nope')\n",
    "        \n",
    "    \n",
    "vic_f = [get_vics(summary) for summary in df['clean'].values]                          "
   ]
